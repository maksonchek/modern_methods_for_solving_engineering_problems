@prefix : <http://example.org/business#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://example.org/business> a owl:Ontology ;
    rdfs:label "Simple Business Ontology (example)"@en .

### Classes
:Company a owl:Class ;
    rdfs:label "Company"@en .

:Bank a owl:Class ;
    rdfs:subClassOf :Company ;
    rdfs:label "Bank"@en .

:Restaurant a owl:Class ;
    rdfs:subClassOf :Company ;
    rdfs:label "Restaurant"@en .

:Employee a owl:Class ;
    rdfs:label "Employee"@en .

:Manager a owl:Class ;
    rdfs:subClassOf :Employee ;
    rdfs:label "Manager"@en .

:Developer a owl:Class ;
    rdfs:subClassOf :Employee ;
    rdfs:label "Developer"@en .

:Chef a owl:Class ;
    rdfs:subClassOf :Employee ;
    rdfs:label "Chef"@en .

:Product a owl:Class ;
    rdfs:label "Product"@en .

:Service a owl:Class ;
    rdfs:subClassOf :Product ;
    rdfs:label "Service"@en .

:FoodItem a owl:Class ;
    rdfs:subClassOf :Product ;
    rdfs:label "FoodItem"@en .

:Software a owl:Class ;
    rdfs:subClassOf :Product ;
    rdfs:label "Software"@en .

:Department a owl:Class ;
    rdfs:label "Department"@en .

:IT a owl:Class ;
    rdfs:subClassOf :Department ;
    rdfs:label "IT Department"@en .

:Kitchen a owl:Class ;
    rdfs:subClassOf :Department ;
    rdfs:label "Kitchen"@en .

### Object properties (relations)
:develops a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Product ;
    rdfs:label "develops"@en ;
    owl:inverseOf :isDevelopedBy ;
    .

:isDevelopedBy a owl:ObjectProperty ;
    rdfs:domain :Product ;
    rdfs:range :Company ;
    rdfs:label "is developed by"@en .

:works_in a owl:ObjectProperty ;
    rdfs:domain :Employee ;
    rdfs:range :Department ;
    rdfs:label "works in"@en ;
    owl:inverseOf :hasEmployee ;
    .

:hasEmployee a owl:ObjectProperty ;
    rdfs:domain :Department ;
    rdfs:range :Employee ;
    rdfs:label "has employee"@en .

:manages a owl:ObjectProperty ;
    rdfs:domain :Employee ;
    rdfs:range :Department ;
    rdfs:label "manages"@en ;
    owl:inverseOf :isManagedBy ;
    a owl:FunctionalProperty ;
    .

:isManagedBy a owl:ObjectProperty ;
    rdfs:domain :Department ;
    rdfs:range :Employee ;
    rdfs:label "is managed by"@en ;
    a owl:InverseFunctionalProperty ;
    .

:belongs_to a owl:ObjectProperty ;
    rdfs:domain :Product ;
    rdfs:range :Company ;
    rdfs:label "belongs to"@en ;
    owl:inverseOf :hasProduct ;
    a owl:FunctionalProperty ;
    .

:hasProduct a owl:ObjectProperty ;
    rdfs:domain :Company ;
    rdfs:range :Product ;
    rdfs:label "has product"@en ;
    a owl:InverseFunctionalProperty ;
    .

### Datatype properties (attributes)
:name a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Company :Employee :Product :Department ) ] ;
    rdfs:range xsd:string ;
    rdfs:label "name"@en .

:price a owl:DatatypeProperty ;
    rdfs:domain :Product ;
    rdfs:range xsd:decimal ;
    rdfs:label "price"@en .

:position a owl:DatatypeProperty ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string ;
    rdfs:label "position"@en .

:location a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:string ;
    rdfs:label "location"@en .

### Individuals (instances) - >=5 entities
# Companies
:TechSolutions a :Company ;
    :name "TechSolutions"@en ;
    rdfs:label "TechSolutions"@en ;
    :hasProduct :MobileApp ;
    :hasProduct :CloudService .

:FoodMaster a :Restaurant ;
    :name "FoodMaster"@en ;
    rdfs:label "FoodMaster"@en ;
    :hasProduct :OrganicJuice ;
    :hasProduct :SeasonalSalad .

:CityBank a :Bank ;
    :name "CityBank"@en ;
    rdfs:label "CityBank"@en ;
    :hasProduct :BankAccount ;
    :hasProduct :LoanService .

# Departments
:ITDept a :IT ;
    :name "IT Department"@en ;
    rdfs:label "IT Department"@en .

:KitchenDept a :Kitchen ;
    :name "Kitchen"@en ;
    rdfs:label "Kitchen"@en .

# Employees
:Anna a :Manager ;
    :name "Anna Petrov"@en ;
    :position "Head of IT"@en ;
    :works_in :ITDept ;
    :manages :ITDept .

:Ivan a :Developer ;
    :name "Ivan Sidorov"@en ;
    :position "Backend Developer"@en ;
    :works_in :ITDept ;
    :works_in :KitchenDept .

:Olga a :Chef ;
    :name "Olga Ivanova"@en ;
    :position "Head Chef"@en ;
    :works_in :KitchenDept ;
    :manages :KitchenDept .

:Sergey a :Teller ;
    :name "Sergey Kuznetsov"@en ;
    :position "Bank Teller"@en ;
    :works_in :ITDept .

# Products / Services
:MobileApp a :Software ;
    :name "MobileApp"@en ;
    :price "1000"^^xsd:decimal ;
    :belongs_to :TechSolutions ;
    rdfs:label "Mobile Application"@en .

:CloudService a :Service ;
    :name "Cloud Hosting"@en ;
    :price "5000"^^xsd:decimal ;
    :belongs_to :TechSolutions ;
    rdfs:label "Cloud Service"@en .

:OrganicJuice a :FoodItem ;
    :name "Organic Juice"@en ;
    :price "5"^^xsd:decimal ;
    :belongs_to :FoodMaster ;
    rdfs:label "Organic Juice"@en .

:SeasonalSalad a :FoodItem ;
    :name "Seasonal Salad"@en ;
    :price "8"^^xsd:decimal ;
    :belongs_to :FoodMaster ;
    rdfs:label "Seasonal Salad"@en .

:BankAccount a :Service ;
    :name "Checking Account"@en ;
    :price "0"^^xsd:decimal ;
    :belongs_to :CityBank ;
    rdfs:label "Bank Account"@en .

:LoanService a :Service ;
    :name "Personal Loan"@en ;
    :price "0"^^xsd:decimal ;
    :belongs_to :CityBank ;
    rdfs:label "Loan Service"@en .

